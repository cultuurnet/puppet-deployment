[Unit]
Description=UiTdatabank search API consume api queue
After=syslog.target network.target remote-fs.target nss-lookup.target

[Service]
User=www-data
Group=www-data
PIDFile=/var/run/udb3-consume-api.pid
WorkingDirectory=<%= @basedir %>
ExecStart=/usr/bin/php bin/app.php consume-udb3-api
Restart=on-failure
RestartSec=10s
Type=simple

[Install]
WantedBy=multi-user.target
