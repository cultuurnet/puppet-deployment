[Unit]
Description=UiTdatabank AMQP listener RDF queue
After=syslog.target network.target remote-fs.target nss-lookup.target

[Service]
User=www-data
Group=www-data
PIDFile=/var/run/udb3-amqp-listener-rdf.pid
WorkingDirectory=<%= @basedir %>
ExecStart=/usr/bin/php bin/udb3.php amqp-listen-rdf
Restart=on-failure
RestartSec=10s
Type=simple

[Install]
WantedBy=multi-user.target
